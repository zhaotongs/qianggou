<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bdhlife.mapper.CarouselMapper">

    <select id="findCarouselList" resultType="com.bdhlife.entity.Carousel">
        select
        id as id,
        url as url,
        name as name,
        create_time as createTime,
        goods_id as goodsId
        from
        tb_carousel
    </select>

</mapper>